{
	"name": "@fineract-apps/fineract-api",
	"version": "0.1.0",
	"private": true,
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"scripts": {
		"codegen": "pnpm codegen:fineract && pnpm codegen:customer-service",
		"codegen:fineract": "openapi-rq -i ./fineract.json -o ./src/generated -c axios && node scripts/add-ts-nocheck.mjs",
		"codegen:customer-service": "openapi-rq -i ./customer-service.json -o ./src/generated/customer-service -c axios && node scripts/add-ts-nocheck.mjs"
	},
	"dependencies": {
		"axios": "^1.12.2"
	},
	"peerDependencies": {
		"@tanstack/react-query": "*",
		"react": "*"
	},
	"devDependencies": {
		"@7nohe/openapi-react-query-codegen": "^1.6.2",
		"@tanstack/react-query": "^5.90.2",
		"react": "^19.1.1",
		"typescript": "^5.5.3"
	}
}
